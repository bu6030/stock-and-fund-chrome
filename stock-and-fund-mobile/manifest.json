{
  "manifest_version": 3,
  "version": "1.0",
  "name": "股票基金神器",
  "description": "股票基金神器，添加股票基金，实时盯盘，实时计算盈亏，添加股票基金",
  "action": {
    "default_title": "股基神器",
    "default_popup": "popup.html"
  },
  "permissions": [
    "bookmarks",
    "background",
    "storage",
    "tabs"
  ],
  "content_scripts": [
      {
          "matches": ["https://*.google.com/*", "https://google.com/*"],
          "js": ["js/jquery.min.js", "js/BigDecimal.js", "js/bootstrap.min.js", "js/stockAndFund.js"],
          "css": ["css/bootstrap.min.css"],
          "run_at": "document_start"
      }
  ],
  "web_accessible_resources": [
    {
      "resources": ["js/*","css/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
